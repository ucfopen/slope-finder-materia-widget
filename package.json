{
  "name": "@ucfcdl/slope-finder",
  "materia": {
    "cleanName": "slope-finder"
  },
  "dependencies": {
    "jquery": "1.12.3",
    "jsxgraph": "^0.99.7",
    "materia-widget-development-kit": "^2.0.0",
    "mathquill": "0.9.1"
  },
  "scripts": {
    "start": "webpack-dev-server",
    "build-dev": "webpack",
    "build": "webpack -p",
    "test": "echo 'no tests implemented'",
    "test-ci": "echo 'no tests implemented'"
  },
  "jest": {
    "coverageReporters": [
      "text",
      "lcov"
    ],
    "verbose": false,
    "transform": {
      "^.+\\.coffee$": "jest-coffee-preprocessor"
    },
    "moduleFileExtensions": [
      "coffee",
      "js"
    ],
    "collectCoverageFrom": [
      "src/**/*.coffee"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100
      }
    }
  },
  "version": "1.0.1",
  "author": "University of Central Florida, Center for Distributed Learning",
  "description": "Materia widget used to display the slope of a line",
  "license": "AGPL-3.0"
}
